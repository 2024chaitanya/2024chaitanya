#include <stdio.h>

int main(){
    int i, j, k;
    
    printf("Enter three numbers:- ");
    scanf("%d %d %d", &i, &j, &k);
    
    if (j>j && i>k){
        printf("%d is greatest.",i);
    } else if (j>i && j>k){
        printf("%d is greater.", j);
    } else {
        printf("%d is greatest.", k);
    }
    return 0;
    }
